<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ModifyAppointmentController">
   <children>
      <Label layoutX="273.0" layoutY="14.0" text="Modify an Appointment">
         <font>
            <Font size="24.0" />
         </font></Label>
      <HBox layoutX="73.0" layoutY="95.0" prefHeight="370.0" prefWidth="577.0">
         <children>
            <VBox alignment="CENTER" prefHeight="370.0" prefWidth="298.0">
               <children>
                  <Label text="Appointment ID">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="15.0" top="15.0" />
                     </VBox.margin></Label>
                  <Label text="Appointment Type">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="15.0" top="15.0" />
                     </VBox.margin></Label>
                  <Label text="Contact">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="15.0" top="15.0" />
                     </VBox.margin></Label>
                  <Label text="Location">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="15.0" top="15.0" />
                     </VBox.margin></Label>
                  <Label text="Date">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="15.0" top="15.0" />
                     </VBox.margin></Label>
                  <Label text="Time">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="15.0" top="15.0" />
                     </VBox.margin></Label>
               </children></VBox>
            <VBox alignment="CENTER" prefHeight="370.0" prefWidth="305.0">
               <children>
                  <TextField fx:id="idTextField" editable="false">
                     <VBox.margin>
                        <Insets bottom="10.0" top="12.0" />
                     </VBox.margin></TextField>
                  <TextField fx:id="typeTextField">
                     <VBox.margin>
                        <Insets bottom="10.0" top="12.0" />
                     </VBox.margin></TextField>
                  <TextField fx:id="contactTextField">
                     <VBox.margin>
                        <Insets bottom="10.0" top="12.0" />
                     </VBox.margin></TextField>
                  <ComboBox fx:id="cityComboBox" prefWidth="150.0" />
                  <DatePicker fx:id="appointmentDatePicker">
                     <VBox.margin>
                        <Insets bottom="10.0" top="12.0" />
                     </VBox.margin></DatePicker>
                  <TextField fx:id="timeTextField">
                     <VBox.margin>
                        <Insets bottom="10.0" top="12.0" />
                     </VBox.margin></TextField>
               </children></VBox>
         </children>
      </HBox>
      <Button fx:id="modifyButton" layoutX="671.0" layoutY="144.0" mnemonicParsing="false" onAction="#modifyButtonClicked" prefHeight="67.0" prefWidth="113.0" text="Modify" />
      <Button fx:id="backButton" layoutX="671.0" layoutY="250.0" mnemonicParsing="false" onAction="#backButtonClicked" prefHeight="67.0" prefWidth="113.0" text="Back" />
   </children>
</AnchorPane>
