<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="624.0" prefWidth="969.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ReportsController">
   <children>
      <Label layoutX="444.0" layoutY="14.0" text="Reports">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextArea fx:id="reportTextArea" layoutX="37.0" layoutY="67.0" prefHeight="470.0" prefWidth="895.0" />
      <HBox alignment="CENTER" layoutX="37.0" layoutY="553.0" prefHeight="44.0" prefWidth="632.0">
         <children>
            <RadioButton fx:id="appointmentMonthRadioButton" mnemonicParsing="false" text="Appointment type by month">
               <HBox.margin>
                  <Insets left="20.0" right="20.0" />
               </HBox.margin></RadioButton>
            <RadioButton fx:id="consultantScheduleRadioButton" mnemonicParsing="false" text="Consultant Schedules">
               <HBox.margin>
                  <Insets left="20.0" right="20.0" />
               </HBox.margin></RadioButton>
            <RadioButton fx:id="appointmentCustomerRadioButton" mnemonicParsing="false" text="Total Appointments by Customer">
               <HBox.margin>
                  <Insets left="20.0" right="20.0" />
               </HBox.margin></RadioButton>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="675.0" layoutY="545.0" prefHeight="61.0" prefWidth="259.0">
         <children>
            <Button fx:id="clearButton" mnemonicParsing="false" onAction="#clearTextArea" prefHeight="50.0" prefWidth="100.0" text="Clear Report">
               <HBox.margin>
                  <Insets left="20.0" right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonClicked" prefHeight="50.0" prefWidth="100.0" text="Back">
               <HBox.margin>
                  <Insets left="10.0" right="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
